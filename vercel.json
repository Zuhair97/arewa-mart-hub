{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://app-cdn.pinetwork.com/ https://*.pinetwork.com https://minepi.com/;"
        }
      ]
    }
  ]
}