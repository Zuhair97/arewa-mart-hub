{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://app-cdn.pinetwork.com/"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://app-cdn.pinetwork.com/ https://minepi.com/;"
        }
      ]
    }
  ]
}